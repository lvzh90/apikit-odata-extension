#%RAML 1.0
title: Auto-generated RAML for DataGateway
version: 0.1
mediaType:  application/json
uses:
  - odata: !include libraries/odata.raml

resourceTypes:
  - collection: !include resources/collection.raml
  - member: !include resources/member.raml

types:
  gateways:
    (odata.remote): gateways
    properties:
  users:
    (odata.remote): users
    properties:
      last_name:
        (odata.nullable): false
        type: string
        (odata.sample): 12
        (odata.key): false
      id:
        (odata.nullable): false
        type: integer
        (odata.sample): 12
        (odata.key): true
      first_name:
        (odata.nullable): false
        type: string
        (odata.sample): 12
        (odata.key): false
      email:
        (odata.nullable): false
        type: string
        (odata.sample): 12
        (odata.key): false
/gateways:
    displayName: gateways
    type: { collection: { model: model.gateways } }
    /{gatewaysId}:
        displayName: gateways Id
        type: { member: { model: model.gateways } }
/users:
    displayName: users
    type: { collection: { model: model.users } }
    /{usersId}:
        type: { member: { model: model.users } }